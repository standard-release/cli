{
  "name": "@tunnckocore/release-cli",
  "description": "Publish new project to NPM following Conventional Commits specification and SemVer. Uses [git-commits-since][] for detecting git commits since last SemVer tag and what next version should be.",
  "license": "Apache-2.0",
  "licenseStart": "2018",
  "scripts": {
    "docs": "docks --outfile .verb.md && verb",
    "lint": "eslint src test index.js cli.js --cache --fix --quiet --format codeframe",
    "test": "nyc asia",
    "commit": "yarn lint && yarn test && yarn dry",
    "dry": "git add -A && git status --porcelain && gitcommit -sS",
    "release": "node cli.js"
  },
  "engines": {
    "node": "^8.11.0 || >=10.13.0"
  },
  "dependencies": {
    "@tunnckocore/execa": "^2.1.2",
    "dedent": "^0.7.0",
    "esm": "^3.0.84",
    "git-commits-since": "^1.0.2",
    "is-ci": "^1.2.1",
    "mri": "^1.1.1"
  },
  "devDependencies": {
    "asia": "^0.19.7",
    "docks": "^0.6.0",
    "eslint": "^5.8.0",
    "eslint-config-esmc": "^1.0.12",
    "gitcommit": "^1.0.13"
  },
  "files": [
    "src",
    "index.js",
    "cli.js"
  ],
  "bin": {
    "release": "cli.js",
    "release-cli": "cli.js",
    "tunnckocore-release": "cli.js"
  },
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "0.0.0",
  "repository": "tunnckoCoreLabs/release-cli",
  "homepage": "https://github.com/tunnckoCore/release-cli",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "charlike",
        "docks",
        "gitcommit",
        "@tunnckocore/execa",
        "@tunnckocore/package-json",
        "@tunnckocore/create-project",
        "@tunnckocore/update",
        "@tunnckocore/config",
        "@tunnckocore/scripts"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
